{
  "configurations": [
    {
      "type": "java",
      "name": "CodeLens (Launch) - KruskalMST",
      "request": "launch",
      "mainClass": "MyjavaTest.KruskalMST",
      "projectName": "Graph",
      "args": ["src/main/data/1000EWG.txt"],
    },
    {
      "type": "java",
      "name": "CodeLens (Launch) - App",
      "request": "launch",
      "mainClass": "MyjavaTest.App",
      "projectName": "Graph"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node.js",
      "program": "${file}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "explore: chrome",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/src",
      "breakOnLoad": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
    
    }
  ]
}